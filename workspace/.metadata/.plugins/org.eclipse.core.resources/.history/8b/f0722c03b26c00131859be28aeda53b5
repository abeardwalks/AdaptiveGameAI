package players;

import interfaces.IntPairInterface;
import interfaces.PlayerInterface2;

public class Human2 implements PlayerInterface2 {

	private char colour;
	
	@Override
	public int placeToken(String state) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int removeToken(String state) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public IntPairInterface moveToken(String state) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public String getName() {
		return "Human";
	}

	@Override
	public void setTokenColour(char c) {
		colour = c;
	}

	@Override
	public char getTokenColour() {
		return colour;
	}

}
